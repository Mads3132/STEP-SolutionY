<mvc:View
	controllerName="schema.TimeRegistry.controller.App"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">

	<App id="app" />

	<Input 
	value="{}"
	description="Expected work hours:"
	valueLiveUpdate="true"
	width="60%"
	type="Number"
	/>

	<PlanningCalendar
	width="60%"
	height="30%"
	firstDayOfWeek="1"
	></PlanningCalendar>
	
</mvc:View>