{
  "name": "TimeRegistry",
  "version": "1.0.0",
  "description": "UI5 Application: TimeRegistry",
  "author": "Rosengr3en",
  "license": "Apache-2.0",
  "scripts": {
    "build": "ui5 build --clean-dest",
    "build:opt": "ui5 build self-contained --clean-dest --all",
    "start:ui5": "ui5 serve --port 8080 -o index.html",
    "start:dist": "ui5 serve  --port 8080 -o index.html --config ui5-dist.yaml",
    "ts-typecheck": "tsc --noEmit",
    "lint": "eslint webapp"
  },
  "devDependencies": {
    "@sapui5/ts-types-esm": "1.108.10",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@ui5/cli": "^3.0.4",
    "eslint": "^8.27.0",
    "typescript": "^4.8.4",
    "ui5-middleware-livereload": "^0.7.0",
    "ui5-middleware-simpleproxy": "^0.8.4",
    "ui5-tooling-transpile": "^0.2.0"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-livereload",
      "ui5-tooling-transpile",
      "ui5-middleware-simpleproxy"
    ]
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": "db/database.db"
        }
      }
    }
  }
}
